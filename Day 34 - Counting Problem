#include <iostream>
using namespace std;
int main()
{
    int t;
    cin >> t;
    while (t--)
    {
        int n,count=0,i;
        cin >> n;
        int a[n];

        for (i = 0; i < n; i++)
        {
            cin >> a[i];
            if(a[i]%2==1){
                count++;
            }
        }

        if (count>1 && count % 2 == 0)
            cout << "YES" << endl;
        else
            cout << "NO" << endl;
    }
    return 0;
}
