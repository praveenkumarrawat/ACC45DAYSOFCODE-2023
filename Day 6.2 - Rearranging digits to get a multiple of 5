#include <iostream>
using namespace std;

int main()
{
    int t;
    cin >> t;
    while (t--)
    {
        int d;
        cin >> d;
        string n;
        cin >> n;
        int count = 0;
        for (int i = 0; i < d; i++)
        {
            if (n[i] == '0' || n[i] == '5')
            {
                count = 1;
                break;
            }
        }
        if (count == 1)
        {
            cout << "YES" << endl;
        }
        else
        {
            cout << "NO" << endl;
        }
    }
    return 0;
}
