#include <iostream>
using namespace std;
int main()
{
    int t;
    cin >> t;
    while (t--)
    {
        int n, max = 0;
        cin >> n;
        int h[n];
        for (int i = 1; i <= n; i++)
        {
            cin >> h[i];
            if (h[i] > max)
                max = h[i];
        }
        cout << max << endl;
    }
    return 0;
}
