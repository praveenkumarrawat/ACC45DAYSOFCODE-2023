#include <iostream>
using namespace std;

int main()
{
    int t;
    cin >> t;
    int x, y, z;
    while (t--)
    {
        cin >> x >> y >> z;
        if (x+y==z || y+z==x || z+x==y)
        {
            cout << "yes" << endl;
        }
        else
            cout << "no" << endl;
    }
    return 0;
}
