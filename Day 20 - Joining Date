#include <iostream>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--)
	{
	    int n,k;
	    cin>>n>>k;
	    int d=n%5;
	    int c=k%5;
	    if(d==0 && c==0)
	        cout<<n/5-k/5<<endl;
	    else if(d==0 && c!=0)
	        cout<<n/5-k/5-1<<endl;
	    else if(d!=0 && c==0)
	        cout<<n/5-k/5+1<<endl;
	    else
	        cout<<n/5-k/5<<endl;
	}
	return 0;
}
